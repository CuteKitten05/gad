Public Class Form1
    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
        Dim fact, i, r As Integer
        fact = Val(TextBox1.Text)
        For i = 1 To fact - 1
            fact = fact * i
        Next
        MsgBox("factorial :" & fact, vbInformation, "factorail")
    End Sub
End Class
